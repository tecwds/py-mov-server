<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.wpaint.pymov.mapper.OccupationDictMapper">

    <resultMap id="BaseResultMap" type="top.wpaint.pymov.domain.OccupationDict">
            <id property="occupationId" column="occupation_id" />
            <result property="enName" column="en_name" />
            <result property="cnName" column="cn_name" />
    </resultMap>

    <resultMap id="OccupationVoList" type="top.wpaint.pymov.domain.vo.OccupationVo">
        <id property="occupationId" column="occupation_id" />
        <result property="enName" column="en_name" />
        <result property="cnName" column="cn_name" />
    </resultMap>

    <sql id="Base_Column_List">
        occupation_id,en_name,cn_name
    </sql>

    <select id="selectOneByOccupationId" resultType="java.lang.Integer">
        select count(occupation_id) from pv_occupation_dict where occupation_id = ${occupationId}
    </select>

    <select id="listAll" resultMap="OccupationVoList">
        SELECT occupation_id, en_name, cn_name FROM pv_occupation_dict
    </select>
</mapper>
